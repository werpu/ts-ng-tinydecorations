{
  "name": "ts-ng-tinydecorations",
  "private": false,
  "version": "0.5.14",
  "description": "A set of annotations for Angular 1.5+",
  "repository": "https://github.com/werpu/ts-ng-tinydecorations",
  "keywords": [
    "Angular",
    "Angular 1.5",
    "Decorations"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.6.25",
    "@types/angular-mocks": "^1.5.10",
    "@types/angular-resource": "^1.5.9",
    "@types/angular-route": "^1.3.4",
    "@types/jasmine": "^2.5.53",
    "copyfiles": "^1.2.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.6.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.2",
    "karma-systemjs": "^0.16.0",
    "npm": "^5.1.0",
    "protractor": "^4.0.9",
    "run": "^1.4.0",
    "systemjs": "^0.19.47",
    "traceur": "0.0.111",
    "typescript": "^2.4.1",
    "webdriver-manager": "^12.0.6"
  },
  "scripts": {
    "update-deps": "npm update",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -i -c-1 ./example",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "build-tslib": "echo \"compiling library\" && tsc --project src/lib/typescript/tsconfig_build.json",
    "build-app": "echo \"compiling app\" && tsc --project src/tsconfig.json",
    "build": "npm run build-tslib && npm run build-app && copyfiles -u 3 ./src/lib/typescript/TinyDecorations.ts ./dist/ && copyfiles -u 3 ./src/lib/typescript/Cache.ts ./dist/  && copyfiles -u 3 ./src/lib/typescript/Routing.ts ./dist/"
  }
}
