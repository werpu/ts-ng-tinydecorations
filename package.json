    "angular-route": "^1.6.4",
    "bower": "^1.7.7",
    "gulp": "^3.9.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.6.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
