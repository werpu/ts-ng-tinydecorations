{
  "name": "ts-ng-tinydecorations",
  "private": false,
  "version": "0.10.4",
  "description": "A set of annotations for Angular 1.5+",
  "repository": "https://github.com/werpu/ts-ng-tinydecorations",
  "keywords": [
    "Angular",
    "Angular 1.5",
    "Decorations"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.6.51",
    "@types/angular-mocks": "^1.7.0",
    "@types/angular-resource": "^1.5.14",
    "@types/angular-route": "^1.7.0",
    "@types/jasmine": "^3.3.1",
    "copyfiles": "^1.2.0",
    "http-server": "^0.11.1",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-systemjs": "^0.16.0",
    "npm": "^6.4.1",
    "protractor": "^5.4.1",
    "run": "^1.4.0",
    "systemjs": "^0.19.47",
    "traceur": "0.0.111",
    "typescript": "^2.9.2",
    "webdriver-manager": "^12.1.0"
  },
  "scripts": {
    "update-deps": "npm update",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -i -c-1 ./example",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "build-tslib": "echo \"compiling library\" && tsc --project src/lib/typescript/tsconfig_build.json",
    "build-tslib-common": "echo \"compiling library\" && tsc --project src/lib/typescript/tsconfig_build_common.json",
    "build-app": "echo \"compiling app\" && tsc --project src/tsconfig.json",
    "build": "npm run build-tslib && npm run build-tslib-common && npm run build-app && copyfiles -u 3 ./src/lib/typescript/TinyDecorations.ts ./dist/ && copyfiles -u 3 ./src/lib/typescript/Cache.ts ./dist/  && copyfiles -u 3 ./src/lib/typescript/Routing.ts ./dist/"
  }
}
